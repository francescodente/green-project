{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PasswordValidationSettings": {
    "MinimumLength": 8,
    "MaximumLength": 64
  },
  "ImageUploadSettings": {
    "DefaultFormat": "Png",
    "Categories": {
      "Size": {
        "Width": 800,
        "Height": 800
      },
      "DirectoryTemplate": "uploads/images/categories/{0}/",
      "Name": "main",
      "Format": "Png"
    },
    "Products": {
      "Size": {
        "Width": 800,
        "Height": 800
      },
      "DirectoryTemplate": "uploads/images/products/{0}/",
      "Name": "main",
      "Format": "Png"
    },
    "SupplierLogos": {
      "Size": {
        "Width": 800,
        "Height": 800
      },
      "DirectoryTemplate": "uploads/images/suppliers/{0}/",
      "Name": "logo",
      "Format": "Png"
    },
    "SupplierBackgroundImages": {
      "Size": {
        "Width": 1920,
        "Height": 1080
      },
      "DirectoryTemplate": "uploads/images/suppliers/{0}/",
      "Format": "Png"
    }
  }
}
